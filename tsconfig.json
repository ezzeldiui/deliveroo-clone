{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "components": ["./src/components/exports.tsx"],
      "ui": ["./src/components/ui/exports.tsx"],
      "lib": ["./src/lib/exports.tsx"],
      "hooks": ["./src/hooks/exports.tsx"],
      "contexts": ["./src/context/exports.tsx"],
      "data": ["./src/data/exports.tsx"],
      "types": ["./src/types/exports.tsx"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
